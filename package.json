{
    "name": "ai-image",
    "version": "1.0.0",
    "private": true,
    "author": "wforguo@gmail.com",
    "description": "AiImage, Chrome plugin, OpenAi Image, ChatGpt",
    "scripts": {
        "dev": "cross-env CRX_ENV=development run-p dev:*",
        "dev:code": "vite build",
        "dev:content": "vite build --config vite.content.config.ts",
        "dev:copy": "node scripts/monitor.js",
        "build": "cross-env CRX_ENV=production pnpm clear && vite build && vite build --config vite.content.config.ts && node scripts/monitor.js",
        "clear": "rimraf extension",
        "preview": "vite preview",
        "format": "prettier .  --write",
        "test:unit": "vitest --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@crxjs/vite-plugin": "^1.0.14",
        "@element-plus/icons-vue": "^2.0.10",
        "axios": "^1.3.4",
        "chrome": "^0.1.0",
        "dayjs": "^1.11.7",
        "element-plus": "^2.2.32",
        "idb": "^7.1.1",
        "openai": "^3.1.0",
        "pinia": "^2.0.32",
        "sass": "^1.58.3",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6",
        "ws": "^8.12.1"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/chrome": "^0.0.217",
        "@types/jsdom": "^21.1.0",
        "@types/node": "^18.14.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "consola": "^2.15.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.22.0",
        "eslint-define-config": "^1.5.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^21.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "rimraf": "^4.1.2",
        "typescript": "~4.9.5",
        "unplugin-auto-import": "^0.15.0",
        "unplugin-vue-components": "^0.24.0",
        "vite": "^4.1.4",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-replace": "^0.1.1",
        "vite-plugin-style-import": "^2.0.0",
        "vitest": "^0.25.6",
        "vue-global-api": "^0.4.1",
        "vue-tsc": "^1.1.7"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
