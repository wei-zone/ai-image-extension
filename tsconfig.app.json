{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
    "exclude": ["dist", "src/**/__tests__/*"],
    "compilerOptions": {
        "composite": true,
        "baseUrl": ".",
        "types": [
            "vite/client",
            "element-plus/global",
            "chrome"
        ],
        "paths": {
            "@/*": ["./src/*"]
        },
    }
}
